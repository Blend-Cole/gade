  color: {{ settings.button_text }};
  display: inline-block;
  font-family: 'Open Sans Hebrew', serif;
  font-weight: {{ main_font.weight }};
  font-size: {{ settings.font-size }}px;
  text-decoration: none;
  cursor: pointer;
  margin-bottom: 20px;
  line-height: normal;
  padding: 15px 0;
}

.button:hover,
button:hover,
input[type="submit"]:hover,
input[type="reset"]:hover,
input[type="button"]:hover {
  color: {{ settings.button_text }};
  background: {{ settings.button_hover }};
}

.secondary-button {
  background: {{ settings.secondary_button_color }} !important;
  color: {{ settings.secondary_button_text }} !important;
}

.secondary-button:hover {
  color: {{ settings.secondary_button_text }} !important;
  background: {{ settings.secondary_button_hover }} !important;
}

.button.full-width,
